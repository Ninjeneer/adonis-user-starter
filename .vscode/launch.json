{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Debug Aaapplication",
			"runtimeExecutable": "node",
			"runtimeArgs": [
				"ace.js",
				"serve",
				"--watch",
				"--node-args=\"inspect-brk\""
			],
			"sourceMaps": true,
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			],
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}
